{
  "name": "agent-orchestrator",
  "version": "1.0.0-beta.1",
  "description": "Multi-track orchestration tool that parallelizes AI coding agents (Claude, Codex) across git worktrees",
  "type": "module",
  "bin": {
    "agent-orchestrator": "dist/server/cli.js"
  },
  "files": [
    "dist/",
    "examples/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "vite",
    "build": "vite build && tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "init": "tsx src/server/init.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestrator",
    "claude",
    "codex",
    "git",
    "worktree",
    "parallel",
    "coding-agent",
    "automation"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@rollup/rollup-linux-arm64-gnu": "^4.57.1",
    "better-sqlite3": "^11.7.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "nanoid": "^5.0.9",
    "proper-lockfile": "^4.1.2",
    "socket.io": "^4.8.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "lucide-react": "^0.460.0",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.0",
    "socket.io-client": "^4.8.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "zustand": "^5.0.0"
  }
}
